<!-- 
Print a message in the console using Group() Method
-->



HTML :  


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script src="03-main.js"></script>
</body>
</html>


JS : 


console.group("Front End");
console.log("1st HTML");
console.log("2nd CSS");
console.log("3nd JavaScript");

console.group("Front End - Developer Mobile");
console.log("1st Learn kotlin");
console.log("2nd Learn React");
console.log("3nd Practces Languages");
console.groupEnd();
console.groupEnd();


console.group("Back End")
console.log("1st Basics Of Algorithms logic");
console.log("2nd Learn Python");
console.log("3nd Learn C or Java");
